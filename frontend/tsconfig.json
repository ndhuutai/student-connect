{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "removeComments": true,
    "noEmit": true,
    "baseUrl": "/"
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
